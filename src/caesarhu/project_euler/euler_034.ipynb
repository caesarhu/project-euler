   "source": [
    "function is_curious(n::Int64)\n",
    "    map(factorial, digits(n)) |> x -> reduce(+, x) |> y -> y == n\n",
    "end\n",
    "\n",
    "function solve_034()\n",
    "    filter(is_curious, 3:50000) |> x -> reduce(+, x)\n",
    "end"
   ]
