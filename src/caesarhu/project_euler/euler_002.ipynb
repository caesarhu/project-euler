   "source": [
    "function sum_fib(limit::Int64, sum::Int64, a::Int64, b::Int64)::Int64\n",
    "    if b > limit\n",
    "        sum\n",
    "    else\n",
    "        sum_fib(limit, (iseven(b) ? sum+b : sum), b, a+b)\n",
    "    end\n",
    "end\n",
    "\n",
    "function solve_002(limit=4000000)\n",
    "    sum_fib(limit, 0, 1, 1)\n",
    "end\n",
    "\n",
    "@time solve_002()"
   ]
  }
